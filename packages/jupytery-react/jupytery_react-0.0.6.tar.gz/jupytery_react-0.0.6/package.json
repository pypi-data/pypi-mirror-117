{
  "name": "@datalayer-jupyter/react-top",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "build:storybook": "lerna run --scope @datalayer/jupyter-storybook --parallel build:storybook",
    "build:vercel": "yarn && yarn build && yarn build:storybook",
    "clean": "rimraf node_modules lib build",
    "deploy": "lerna run deploy",
    "example:jupyter:widgets": "lerna run --scope @datalayer-jupyter/* --parallel example:jupyter:widgets",
    "example:lumino": "lerna run --scope @datalayer-jupyter/* --parallel example:lumino",
    "storybook:jupyter": "lerna run --scope @datalayer-jupyter/* --parallel storybook",
    "test": "lerna run test"
  },
  "workspaces": {
    "packages": [
      "examples/*",
      "examples/*/*",
      "experiments/*",
      "packages/*",
      "packages/*/*",
      "storybook"
    ],
    "nohoist": [
    ]
  },
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0"
  },
  "devDependencies": {
    "concurrently": "6.2.0",
    "jest-prop-type-error": "^1.1.0",
    "lerna": "^3.10.7",
    "prettier": "2.3.1",
    "vercel": "21.3.3"
  },
  "resolutions": {
    "**/@types/react": "17.0.6",
    "**/@types/react-dom": "17.0.5",
    "**/react": "17.0.2",
    "**/react-dom": "17.0.2",
    "**/typescript": "4.2.4",
    "**/html-webpack-plugin": "5.3.1",
    "**/webpack": "5.37.1",
    "**/webpack-cli": "4.7.0",
    "**/webpack-dev-server": "4.0.0-beta.3",
    "**/webpack-virtual-modules": "^0.4.2"
  }
}
