{
  "name": "@datalayer-jupyter/css",
  "version": "0.0.5",
  "license": "MIT",
  "private": true,
  "author": "Datalayer",
  "homepage": "https://github.com/datalayer/datalayer/lab/css#README",
  "bugs": {
    "url": "https://github.com/datalayer/datalayer/issues"
  },
  "scripts": {
    "watch": "concurrently --kill-others \"yarn tsc-watch\" \"yarn serve-watch\"",
    "tsc-watch": "tsc --watch",
    "serve-watch": "webpack serve --open --watch",
    "build": "webpack",
    "css-deps": "./dev/remove-tilde.sh && node dev/css.js",
    "css": "node dev/css.js --trace-warnings",
    "dev": "webpack --progress --colors --watch --env dev",
    "start": "webpack serve --open",
    "start-watch": "webpack serve --open --watch",
    "clean": "rimraf lib dist",
    "clean-all": "rimraf lib dist node_modules",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datalayer/datalayer.git"
  },
  "dependencies": {
    "@jupyterlab/theme-dark-extension": "^3.0.7",
    "@jupyterlab/theme-light-extension": "^3.0.7",
    "@jupyterlab/ui-components": "^3.0.4",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "7.14.5",
    "@types/jest": "^26.0.23",
    "@types/node": "15.6.1",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/react-jss": "10.0.0",
    "@types/webpack": "4.41.12",
    "babel-loader": "8.2.2",
    "codecov": "^3.1.0",
    "concurrently": "6.2.0",
    "css-loader": "5.2.4",
    "file-loader": "~5.0.2",
    "html-webpack-plugin": "5.3.1",
    "jest": "^23.6.0",
    "node-sass": "^4.13.0",
    "postcss": "~7.0.26",
    "postcss-css-variables": "0.14.0",
    "postcss-import": "~12.0.1",
    "postcss-merge-rules": "~4.0.3",
    "postcss-omit-import-tilde": "~1.0.1",
    "prettier": "2.3.1",
    "raw-loader": "~4.0.0",
    "react-jss": "10.3.0",
    "rimraf": "3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "2.0.0",
    "svg-url-loader": "~3.0.3",
    "ts-jest": "^26.5.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "4.2.2",
    "url-loader": "~3.0.0",
    "webpack": "5.37.1",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js.map",
    "lib/**/*.js",
    "style/**/*.css",
    "themes/**/*.*"
  ],
  "sideEffects": [
    "style/*.css"
  ],
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  }
}
